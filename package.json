{
  "name": "ralph",
  "version": "0.1.0",
  "description": "Remote and Local Personal hosting",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0"
  },
  "scripts": {
